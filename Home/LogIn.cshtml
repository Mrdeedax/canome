@model canome0._1.Models.LogIn
@{
    ViewBag.Title = "LogIn";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Log in</h2>
<hr />
<form>
    <div class="row">
        <div class="col-md-3">
            <label for="userEmail" class="form-label">Email address</label>
            <input type="email" class="form-control" id="userEmail" aria-describedby="emailHelp">
        </div>
    </div>
    <p></p>
    <div class="row">
        <div class="col-md-3">
            <label for="userPassword" class="form-label">Password</label>
            <input type="password" class="form-control" id="userPassword">
        </div>
    </div>
    <p></p>
    <div class="row">
        <div class="col-md-3">
            <a class="btn btn-primary" href="../Home/profile" role="button">Submit</a>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <ul class="nav">
                <li class="nav-item">
                    <p>
                        <a class="nav-link active" aria-current="page" href="#">Forgot password?</a>
                        Don't have an account?
                        <a class="nav-link active" aria-current="page" href="../Home/SignUp">Sing up</a>
                    </p>
                </li>
            </ul>
        </div>
    </div>
</form>
